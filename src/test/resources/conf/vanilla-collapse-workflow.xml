<com.kmwllc.brigade.config.WorkflowConfig>
    <name>ingest</name>

    <stageConfigs>

        <com.kmwllc.brigade.config.StageConfig>
            <stageName>collapseChildren</stageName>
            <stageClass>com.kmwllc.brigade.stage.CollapseChildrenToParentDoc</stageClass>
            <config>
            </config>
        </com.kmwllc.brigade.config.StageConfig>


        <com.kmwllc.brigade.config.StageConfig>
            <stageName>dumpDoc</stageName>
            <stageClass>com.kmwllc.brigade.stage.DumpDoc</stageClass>
            <config>
                <entry>
                    <string>output</string>
                    <string>csv-test-output.txt</string>
                </entry>
            </config>
        </com.kmwllc.brigade.config.StageConfig>
    </stageConfigs>
    <numWorkerThreads>1</numWorkerThreads>
    <queueLength>50</queueLength>
</com.kmwllc.brigade.config.WorkflowConfig>